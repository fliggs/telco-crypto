model UserKycData {
  id        String   @id @default(uuid()) @db.Uuid
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now()) @updatedAt
  provider  String
  data      Json

  userId String @db.Uuid
  user   User   @relation(fields: [userId], references: [id])

  @@unique([provider, userId])
}
