{
  "cli": {
    "version": ">= 15.0.14",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "environment": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      },
      "pnpm": "10.6.4"
    },
    "preview": {
      "environment": "preview",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "development"
      },
      "pnpm": "10.6.4"
    },
    "production": {
      "environment": "production",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "production"
      },
      "pnpm": "10.6.4"
    }
  },
  "submit": {
    "preview": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      },
      "ios": {
        "ascAppId": "",
        "appleTeamId": ""
      }
    },
    "production": {
      "android": {
        "track": "internal"
      },
      "ios": {
        "ascAppId": "",
        "appleTeamId": ""
      }
    }
  }
}
